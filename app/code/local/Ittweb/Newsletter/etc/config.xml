<?xml version="1.0"?>
<config>
  <modules>
    <Ittweb_Newsletter>
      <version>0.1.0</version>
    </Ittweb_Newsletter>
  </modules>
  <global>
    <helpers>
      <ittweb_newsletter>
        <class>Ittweb_Newsletter_Helper</class>
      </ittweb_newsletter>
    </helpers>
  </global>
  <frontend>
    <routers>
        <ittweb_newsletter>
          <use>standard</use>
          <args>
            <module>Ittweb_Newsletter</module>
            <frontName>ittweb_newsletter</frontName>
          </args>
        </ittweb_newsletter>
    </routers>
  </frontend>
</config> 